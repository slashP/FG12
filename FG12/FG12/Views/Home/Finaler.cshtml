@model FG12.ViewModels.MatchListViewModel
@using FG12.Models
@{
    ViewBag.Title = "Finaler";
}

@if (!(bool)ViewBag.IsQuarterFinalsActivated && Request.IsAuthenticated)
{
    @Html.ActionLink("Generer kvartfinaler", "GenerateKnockoutMatchesQuarterFinals", "KnockoutMatch")
}

@if (!(bool)ViewBag.IsSemiFinalsActivated && Request.IsAuthenticated)
{
    @Html.ActionLink("Generer semifinaler", "GenerateKnockoutMatchesSemiFinals", "KnockoutMatch")
}

@if (!(bool)ViewBag.IsFinalActivated && Request.IsAuthenticated)
{
    @Html.ActionLink("Generer finale", "GenerateKnockoutMatchesFinal", "KnockoutMatch")
}

@Html.Partial("_Matchlists", Model)